        </AnchorPane>
    </AnchorPane>
    <MenuBar fx:id="menuBar" onKeyPressed="#handleKeyInput" prefHeight="25.0" prefWidth="122.0" AnchorPane.bottomAnchor="340.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="0.0">
