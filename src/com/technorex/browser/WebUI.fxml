            <MenuItem text="New" />
            <MenuItem text="Open" />
            <MenuItem text="Save" />
            <MenuItem text="Save As" />
            <SeparatorMenuItem />
            <MenuItem onAction="#close" text="Exit" />
        </Menu>
        <Menu text="Edit">
            <MenuItem text="Copy" />
            <MenuItem text="Cut" />
            <MenuItem text="Paste" />
        </Menu>
        <Menu text="Help">
            <MenuItem disable="true" text="Search" />
            <MenuItem text="Online Manual" visible="false" />
            <SeparatorMenuItem />
            <MenuItem onAction="#handleAboutAction" text="About" />
        </Menu>
    </MenuBar>
    <BorderPane prefHeight="371.0" prefWidth="431.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="30.0">
