<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.web.WebView?>
<AnchorPane id="AnchorPane" prefHeight="371.0" prefWidth="431.0" xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.technorex.browser.WebUIController">
    <AnchorPane id="AnchorPane">
        <AnchorPane id="AnchorPane">
            <GridPane id="GridPane">
                <columnConstraints>
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"/>
                </columnConstraints>
                <rowConstraints>
                    <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
                </rowConstraints>
            </GridPane>
        </AnchorPane>
    </AnchorPane>
    <BorderPane prefHeight="371.0" prefWidth="431.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <center>
            <WebView fx:id="webView" prefHeight="200.0" prefWidth="200.0"/>
        </center>
        <top>
            <HBox prefHeight="27.0" prefWidth="431.0">
                <TextField fx:id="txtURL" onAction="#goAction" prefHeight="27.0" prefWidth="382.0"
                           HBox.hgrow="ALWAYS"/>
                <Button mnemonicParsing="false" onAction="#goAction" prefHeight="27.0" prefWidth="47.0" text="GO"
                        HBox.hgrow="NEVER"/>
            </HBox>
        </top>
    </BorderPane>
</AnchorPane>
