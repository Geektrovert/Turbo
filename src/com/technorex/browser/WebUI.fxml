                  <Insets />
               </HBox.margin></TextField>
                <ImageView fx:id="imageView" fitHeight="29.0" fitWidth="40.0" nodeOrientation="INHERIT" onMousePressed="#goAction" onMouseReleased="#release" pickOnBounds="true">
                    <Image url="@/Icons/Go.png" />
               <HBox.margin>
                  <Insets />
               </HBox.margin>
                </ImageView>
            </HBox>
        </top>
    </BorderPane>
